{
	"root": true,
	"env": {
		"node": true,
		"browser": true
	},
	"extends": [
		"eslint:recommended"
	],
	"parser": "vue-eslint-parser",
	"parserOptions": {
		"sourceType" : "module",
		"ecmaVersion": 2018,
		"ecmaFeatures": {
			"impliedStrict": false,
			"classes": true
			}
		},
	"rules": {
		"indent":"off",
		"strict": 0,
		"no-func-assign":"off",
		"no-mixed-spaces-and-tabs":"off",
		"no-console":"off",
		"no-unused-vars":"off"
	}
}

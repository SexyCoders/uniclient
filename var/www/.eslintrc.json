{
	"root": true,
	"env": {
		"node": true
	},
	"extends": [
		"plugin:vue/vue3-recommended",
		"eslint:recommended"
	],
	"parser": "vue-eslint-parser",
	"parserOptions": {
		"sourceType" : "module",
		"ecmaVersion": 2018,
		"ecmaFeatures": {
			"impliedStrict": false,
			"classes": true
			}
		},
	"rules": {
		"indent":"off"
	}
}
